{
    "name": "jedkirby/valpal-php",
    "description": "A simple API wrapper, written in PHP, for the ValPal API, provided by Angels Media.",
    "keywords": [
        "php",
        "valpal",
        "api",
        "composer"
    ],
    "license": "MIT",
    "homepage": "https://valpal.co.uk",
    "authors": [
        {
            "name": "James Kirby",
            "email": "me@jedkirby.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "jedkirby/php-cs": "^1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jedkirby\\ValPal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jedkirby\\ValPal\\Tests\\": "tests/"
        }
    }
}
